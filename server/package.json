{
    "name": "sequelstring-server",
    "version": "1.0.0",
    "description": "SequelString CRM Backend API Server",
    "main": "dist/index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "build": "tsc",
        "start": "node dist/index.js",
        "worker": "ts-node-dev --respawn src/workers/index.ts",
        "scheduler": "ts-node-dev --respawn src/scheduler/index.ts",
        "test": "jest --coverage",
        "test:watch": "jest --watch",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "type-check": "tsc --noEmit",
        "seed": "ts-node scripts/seed.ts",
        "migrate": "ts-node scripts/migrate.ts"
    },
    "dependencies": {
        "express": "^4.19.2",
        "mongoose": "^8.2.1",
        "ioredis": "^5.3.2",
        "bull": "^4.12.2",
        "socket.io": "^4.7.4",
        "jsonwebtoken": "^9.0.2",
        "bcryptjs": "^2.4.3",
        "zod": "^3.22.4",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "compression": "^1.7.4",
        "morgan": "^1.10.0",
        "winston": "^3.11.0",
        "node-cron": "^3.0.3",
        "openai": "^4.29.2",
        "@sendgrid/mail": "^8.1.0",
        "axios": "^1.6.7",
        "dotenv": "^16.4.5",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.24",
        "@types/cors": "^2.8.17",
        "@types/compression": "^1.7.5",
        "@types/morgan": "^1.9.9",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/bcryptjs": "^2.4.6",
        "@types/node-cron": "^3.0.11",
        "@types/uuid": "^9.0.8",
        "typescript": "^5.3.3",
        "ts-node-dev": "^2.0.0",
        "eslint": "^8.57.0",
        "@typescript-eslint/eslint-plugin": "^7.1.0",
        "@typescript-eslint/parser": "^7.1.0",
        "jest": "^29.7.0",
        "@types/jest": "^29.5.12",
        "ts-jest": "^29.1.2"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}